<?xml version="1.0" ?>
<root xmlns="https://rawgit.com/SOF3/libkinetic/master/libkinetic.xsd">
  <info id="count-blocks" indexName="Count number of blocks in region" synopsis="TODO">
    <enumArgs id="blockTypes" title="Block Types" addItem="Add Block" eachFormat="${blockId}:${blockDamage}"
              submit="Continue" required="true">
      <each title="Choose block" format="${blockId}:${blockDamage}">
        <input id="blockId" title="Block ID"/>
        <input id="blockDamage" title="Block damage"/>
        <toggle id="ignoreDamage" title="Ignore block damage" default="true"/>
      </each>
    </enumArgs>
  </info>
</root>
